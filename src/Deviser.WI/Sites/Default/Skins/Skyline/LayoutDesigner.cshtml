
@{
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
    Deviser.Core.Data.Entities.Page currentPage = (Deviser.Core.Data.Entities.Page)ViewBag.AppContext.CurrentPage;
}


@Component.InvokeAsync("ControlBar")
<div class="clearfix"></div>

<div class="page-content-white logged-in">  
   
    <!-- BEGIN CONTAINER -->
    <div class="page-container page-full-width">
        <!-- BEGIN CONTENT -->
        <div class="page-content-wrapper">
            <div class="page-content">
                <!-- BEGIN PAGE HEADER-->
                <div class="page-bar">

                </div>
                <!-- END PAGE HEADER-->
                @RenderBody()
            </div>
        </div>
        <!-- END CONTENT -->
    </div>
    <!-- END CONTAINER -->
    <!-- BEGIN FOOTER -->
    <div class="page-footer">
        <div class="page-footer-inner">
            @DateTime.Now.Year &copy; Deviser WCM <a href="http://skydevise.com" title="SKY Devise" target="_blank">SKY Devise</a>
        </div>
        <div class="scroll-to-top">
            <i class="icon-arrow-up"></i>
        </div>
    </div>
    <!-- END FOOTER -->
</div>
