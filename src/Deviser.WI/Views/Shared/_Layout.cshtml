<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <title>@ViewData["Title"]</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <meta name="DESCRIPTION" content="@ViewData["Description"]" />
    <meta name="KEYWORDS" content="@ViewData["Keywords"]" />
    <meta name="ROBOTS" content="INDEX, FOLLOW" />

    <link rel="apple-touch-icon" sizes="120x120" href="/images/favicons/apple-touch-icon.png">
    <link rel="icon" type="image/png" sizes="32x32" href="/images/favicons/favicon-32x32.png">
    <link rel="icon" type="image/png" sizes="16x16" href="/images/favicons/favicon-16x16.png">
    <link rel="manifest" href="/images/favicons/site.webmanifest">
    <link rel="mask-icon" href="/images/favicons/safari-pinned-tab.svg" color="#5bbad5">
    <meta name="msapplication-TileColor" content="#ec3d28">
    <meta name="theme-color" content="#ffffff">

    <render type="Css"></render>

    <link rel="stylesheet" href="~/css/admin/controlbar.css" asp-append-version="true" />

    <render type="Script" location="Head"></render>
</head>
<body class="header-fixed">
    <div>
        <render type="Script" location="BodyStart"></render>

        @RenderBody()


        @RenderSection("scripts", required: false)
        <render type="Script" location="BodyEnd"></render>
    </div>
</body>
</html>
